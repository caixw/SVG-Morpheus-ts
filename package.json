{
  "name": "svg-morpheus-ts",
  "prettyName": "SVG Morpheus",
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "description": "ESM TypeScript library enabling SVG icons to morph from one to the other. It implements Material Design's Delightful Details transitions. Refactored with modern TypeScript + Vite + pnpm stack. Supports both Chinese and English documentation.",
  "homepage": "https://github.com/adoin/SVG-Morpheus-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/adoin/SVG-Morpheus-ts.git"
  },
  "license": "MIT",
  "author": {
    "name": "Adoin",
    "email": "adoin@qq.com"
  },
  "keywords": [
    "svg",
    "animation",
    "morphing",
    "icons",
    "material-design"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "demo": "pnpm build && vite --mode demo --host",
    "build:demo": "pnpm build && vite build --mode demo",
    "preview": "vite preview",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "terser": "^5.40.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  }
}
